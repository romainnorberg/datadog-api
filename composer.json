{
    "name": "romainnorberg/datadog-api",
    "description": "The missing DataDog API PHP Wrapper! A lightweight package to fetch metrics from Datadog",
    "keywords": [
        "datadog",
        "datadog-api"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Romainnorberg\\DataDogApi\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Romainnorberg\\DataDogApi\\Tests\\": "tests"
        }
    },
    "type": "library",
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "netresearch/jsonmapper": "^4.1",
        "symfony/config": "^6.2",
        "symfony/dependency-injection": "^6.2",
        "symfony/http-client": "^6.2",
        "symfony/yaml": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "spatie/phpunit-snapshot-assertions": "^5.0",
        "symfony/var-dumper": "^6.2"
    },
    "authors": [
        {
            "name": "Romain Norberg",
            "email": "romainnorberg@gmail.com"
        }
    ],
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
