{
    "name": "romainnorberg/datadog-api",
    "description": "The missing DataDog API PHP Wrapper! A lightweight package to fetch metrics from Datadog",
    "keywords": [
        "datadog",
        "datadog-api"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Romainnorberg\\DataDogApi\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Romainnorberg\\DataDogApi\\Tests\\": "tests"
        }
    },
    "type": "library",
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "netresearch/jsonmapper": "^2.1",
        "symfony/config": "^4.4",
        "symfony/dependency-injection": "^4.4",
        "symfony/http-client": "^4.4",
        "symfony/yaml": "^4.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "symfony/var-dumper": "^4.4"
    },
    "authors": [
        {
            "name": "Romain Norberg",
            "email": "romainnorberg@gmail.com"
        }
    ],
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
